FRAMEWORK_SEARCH_PATHS = $(inherited) "$(SDKROOT)/Developer/Library/Frameworks" "$(PLATFORM_DIR)/Developer/Library/Frameworks"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/BCBalancedMultilineLabel" "${PODS_ROOT}/Headers/Public/FBSnapshotTestCase"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/BCBalancedMultilineLabel" -isystem "${PODS_ROOT}/Headers/Public/FBSnapshotTestCase"
OTHER_LDFLAGS = $(inherited) -ObjC -l"BCBalancedMultilineLabel" -l"FBSnapshotTestCase" -framework "UIKit" -framework "XCTest"
PODS_ROOT = ${SRCROOT}/Pods